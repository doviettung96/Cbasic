#include <stdio.h>
#include <string.h>



void main(int argc, char* argv[]){
  FILE* f1;
 
  int c;
  char* syntax = "cat";
  char* m1 = "r";
  
  
  if(strcmp(argv[1], syntax) != 0 || argc != 3)
    printf("Wrong syntax. The program now will be terminated\n");
  else
    {
      if((f1 = fopen(argv[2], m1)) == NULL)
        printf("Can't open file %s\n",argv[2]);
         else
           {
             while( (c = fgetc(f1)) != EOF)
          putchar(c);
            
           }
    }
  fclose(f1);
}

